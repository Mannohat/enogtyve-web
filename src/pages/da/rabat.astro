<style>
  .intro {
    margin-bottom: 2rem;
    line-height: 1.7;
    color: var(--text-light);
  }

  .intro strong {
    color: rgba(247, 147, 26, 0.9);
  }

  .category-section {
    margin-bottom: 2rem;
  }

  .category-section h2 {
    color: var(--primary);
    font-size: 1.4rem;
    margin-bottom: var(--space-md);
    padding-bottom: var(--space-sm);
    border-bottom: 1px solid rgba(247, 147, 26, 0.2);
  }

  .partners-grid {
    display: grid;
    grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));
    gap: var(--space-md);
  }

  .partner-card {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    gap: 1rem;
    padding: 2rem 1.5rem;
    border-radius: 12px;
    background: rgba(255, 255, 255, 0.06);
    border: 1px solid rgba(247, 147, 26, 0.15);
    transition: all 0.3s ease;
  }

  .partner-card:hover {
    background: rgba(247, 147, 26, 0.08);
    border-color: var(--primary);
    transform: translateY(-3px);
  }

  .partner-logo {
    max-height: 50px;
    max-width: 80%;
    object-fit: contain;
  }

  .partner-name {
    color: var(--text-light);
    font-size: 0.9rem;
    font-weight: 500;
    opacity: 0.8;
  }

  .cta-section {
    margin-top: 1rem;
    margin-bottom: 2rem;
    padding: 1.25rem 1.5rem;
    background: rgba(247, 147, 26, 0.04);
    border: 1px solid rgba(247, 147, 26, 0.15);
    border-radius: 10px;
    text-align: center;
    color: var(--text-light, #ccc);
    line-height: 1.7;
  }

  .cta-section p {
    margin: 0;
    font-size: 0.92rem;
  }

  .cta-section a {
    color: var(--primary);
  }

  .back-to-top {
    display: block;
    width: fit-content;
    margin: 3rem auto 0 auto;
    font-size: 0.9rem;
    font-weight: 500;
    color: rgba(247, 147, 26, 0.9);
    transition: opacity 0.3s ease;
  }

  .back-to-top:hover {
    opacity: 0.7;
  }

  @media (max-width: 567px) {
    .partners-grid {
      grid-template-columns: repeat(2, 1fr);
    }
  }
</style>